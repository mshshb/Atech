body,h1,h2,h3,h4,h5,h6,ul,li,ol,dl,p{
            margin:0;
            padding:0;
        }
        body{
            background-color:#34495E;
            font-family: 'Open Sans', sans-serif;
        }
        #content>.content-one{
            color:#fff;
            background:url("../../images/bann2.jpg") no-repeat;
            background-size:cover;
        }
        #content>.content-one div.row:first-child .btn{
            color:#fff;
            font-size:1.3em;
            padding:6px 33px;
            border-radius:30px;
            background-image:linear-gradient(rgb(254, 211, 127) 0%, rgb(254, 211, 127) 50%, rgb(244, 203, 122) 51%, rgb(244, 203, 122) 100%);
            transition:all ease 0.3s;
        }
        #content>.content-one div.row:first-child a:hover{
            background:#85D6DE;
        }
        #content>.content-one div.row:first-child{
            padding-top:12%;
            padding-bottom:18%;
        }
        #content>.content-one div.row:first-child h1{
            margin:1% 0;
        }
        #content>.content-two div.container-fluid{
            padding:0;
        }
        #content>.content-two div.item-content{
            padding:2.5% 0;
            text-align:center;
            font-size:1.5em;
            font-weight: 400;
            color:#fff;
        }
        #content>.content-two  .carousel-control.left{
            background:url("images/left.png") rgba(255,255,255,0.3) no-repeat 90%;
            opacity:1;
        }
        #content>.content-two  .carousel-control.right{
            background:url("images/right.png") rgba(255,255,255,0.3) no-repeat 10%;
            opacity:1;
        }
        #content>.content-three .container>.row:first-child{
            padding:8% 1%;
            border-bottom:0.5px solid #758493;
        }
        #content>.content-three .container>.row:first-child .img{
            width:125px;
            height:125px;
            background:url("images/images/img-sprite10.png") no-repeat;
        }
        #content>.content-three .container>.row:first-child>div:nth-child(1) .img{
            background-position:-145px;
        }
        #content>.content-three .container>.row:first-child>div:nth-child(2) .img{
            background-position:-9px;
        }
        #content>.content-three .container>.row:first-child>div:nth-child(3) .img{
            background-position:-305px;
        }
        #content>.content-three .container>.row:first-child .img+h4{
            color:#fff;
            font-size:1.5em;
            font-weight:600;
            margin-top:5%;
            margin-bottom:4%;
        }
        #content>.content-three .container>.row:first-child .img+h4+p{
            color:#6F8091;
            font-size:1.1em;
            line-height:1.68em;
        }
        #content>.content-three .container>.intro{
            color:#6F8091;
            margin:3% 0;
            padding:2% 0;
        }
        #content>.content-three .container>.intro .row{
            margin:20px 0;
        }
        #content>.content-three .container>.intro h3{
            color:#fff;
            padding-left:15px;
        }
        #content>.content-three .container>.intro h4{
            color:#fff;
            font-style:italic;
            margin-bottom:20px;
        }
        #content>.content-three .container>.intro p{
            line-height:1.68em;
        }
        #content>.content-three .container>.intro p+p{
            margin-top:1em;
        }
        #content>.content-three .container>.intro img{
            max-width:100%;
        }
        #content>.content-four{
            background:#85D6DE;
            padding:5% 0;
			width:100%;
        }
		#content>.content-four .item>.row{
			position:relative;
		}
        #content>.content-four img{
            max-width:100%;
        }
		#content>.content-four .item .fill{
			position:absolute;
			top:43%;
			left:1.5%;
			width:100%;
			text-align:center;
			font-size:1.7em;
			color:#666;
		}
		#content>.content-four .carousel-inner .item {
			-webkit-transition: 0.8s ease-in-out left;
			-o-transition: 0.8s ease-in-out left;
			transition: 0.8s ease-in-out left;
		}
        #content>.content-five{
            margin:5% 0;
        }
        #content>.content-five h1{
            width:100%;
            color:#fff;
            margin:2% 0;
            text-align:center;
        }
        #content>.content-five h1+.row>div>ul{
            border:0.5px solid #758493;
            padding:15px 15px 0 15px;
            line-height:2.5em;
            list-style:circle inside;
            color:#fff;
        }
        #content>.content-five h1+.row>div>ul a{
            color:#fff;
            font-style:italic;
            text-decoration:none;
            trasition:all 0.5s linear;
        }
        #content>.content-five h1+.row>div>ul a:hover{
            color:#85D6D9;
        }
        #content>.content-five h1+.row>div:nth-child(2)>div{
            margin:0 3%;
            padding:20px;
            border:0.5px solid #758493;
        }
        #content>.content-five h1+.row>div:nth-child(2)>div>p{
            width:100%;
            height:291px;
            background:#f7f7f7;
        }
		@media all and (transform-3d), (-webkit-transform-3d) {
		  #content>.content-four .carousel-inner .item{
			-webkit-transition: -webkit-transform 0.8s ease-in-out;
				 -o-transition:      -o-transform 0.8s ease-in-out;
					transition:         transform 0.8s ease-in-out;
			}
		 }
        @media (max-width: 1000px){
            #content>.content-three .container>.row:first-child .img+h4{
                font-size:1.15em;
                margin-top:2%;
                margin-bottom:1.2%;
            }
            #content>.content-three .container>.row:first-child .img+h4+p{
                font-size:0.8em;
                line-height:1.68em;
                padding-bottom:0.6%;
            }
            #content>.content-three .container>.intro>.row:last-child>div:last-child{
                width:100%;
            }
            #content>.content-three .container>.intro h3{
                font-size:1.8em;
            }
            #content>.content-three .container>.intro>.row{
                font-size:0.8em;
            }
			#content>.content-four .item .fill{
				font-size:1.5em;
			}
            #content>.content-five h1{
                font-size:1.8em;
            }
            #content>.content-five h1+.row>div>ul a{
                font-size:0.8em;
            }
        }
        @media (max-width: 992px){
            #content>.content-two{
                font-size:0.8em;
            }
            #content>.content-two  .carousel-control{
                width:10%;
            }
			#content>.content-four .item .fill{
				font-size:1.1em;
			}
        }
        @media (max-width: 768px){
            #content>.content-one div.row:first-child h1{
                font-size:30px;
            }
            #content>.content-one div.row:first-child{
                text-align:center;
            }
            #content>.content-two{
                font-size:0.6em;
            }
            #content>.content-two  .carousel-control{
                width:15%;
            }
            #content>.content-three .container>.row:first-child>div{
                text-align:center;
            }
            #content>.content-three .container>.row:first-child>div .img{
                margin:0 auto;
            }
            #content>.content-three .container>.intro>.row:first-child{
                text-align:center;
            }
            #content>.content-three .container>.intro h3{
                font-size:1.4em;
            }
            #content>.content-three .container>.intro h4{
                font-size:1.2em;
            }
            #content>.content-three .container>.intro p{
                font-size:0.8em;
            }
			#content>.content-four .item .fill{
				font-size:0.8em;
			}
        }
		@media (max-width: 600px){
			#content>.content-four .item .fill{
				font-size:1.7em;
			}
		}
        @media (max-width: 480px){
            #content>.content-three .container>.row:first-child>div.col-xs-4{
                width:100%;
            }
            #content>.content-three .container>.row:first-child .img+h4{
                margin-top:6%;
                margin-bottom:2%;
            }
            #content>.content-three .container>.intro h3{
                font-size:1.1em;
            }
            #content>.content-three .container>.intro h4{
                font-size:1em;
            }
            #content>.content-five h1{
                font-size:1.3em;
            }
        }